{
    "name": "bcrowe/cakephp-encrypted-type",
    "description": "CakePHP 4 plugin that provides application-level database encryption.",
    "type": "cakephp-plugin",
    "keywords": [
        "cakephp", "database", "type", "cakephp3", "encrypted", "encryption", "encrypt", "cakephp4"
    ],
    "homepage": "https://github.com/bcrowe/cakephp-encrypted-type",
    "license": "MIT",
    "authors": [
        {
            "name": "Bryan Crowe",
            "email": "bryan@bryan-crowe.com",
            "homepage": "http://bryan-crowe.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "BryanCrowe\\EncryptedType\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BryanCrowe\\EncryptedType\\": "src",
            "BryanCrowe\\EncryptedType\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
